<p id="notice"><%= notice %></p>

<h1>Listing First Tests</h1>

<table>
  <thead>
    <tr>
      <th>Info1</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_tests.each do |first_test| %>
      <tr>
        <td><%= first_test.info1 %></td>
        <td><%= first_test.user %></td>
        <td><%= link_to 'Show', first_test %></td>
        <td><%= link_to 'Edit', edit_first_test_path(first_test) %></td>
        <td><%= link_to 'Destroy', first_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New First test', new_user_first_test_path(@user) %>
