<p id="notice"><%= notice %></p>

<h1>Listing First Reviews</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>First test</th>
      <th>Info1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_reviews.each do |first_review| %>
      <tr>
        <td><%= first_review.user %></td>
        <td><%= first_review.first_test %></td>
        <td><%= first_review.info1 %></td>
        <td><%= link_to 'Show', first_review %></td>
        <td><%= link_to 'Edit', edit_first_review_path(first_review) %></td>
        <td><%= link_to 'Destroy', first_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New First review', new_first_review_path %>
