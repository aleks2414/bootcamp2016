<p id="notice"><%= notice %></p>

<h1>Listing Second Tests</h1>

<table>
  <thead>
    <tr>
      <th>Boceto</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_tests.each do |second_test| %>
      <tr>
        <td><%= second_test.boceto %></td>
        <td><%= second_test.user %></td>
        <td><%= link_to 'Show', second_test %></td>
        <td><%= link_to 'Edit', edit_second_test_path(second_test) %></td>
        <td><%= link_to 'Destroy', second_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second test', new_second_test_path %>
