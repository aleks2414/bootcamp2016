<p id="notice"><%= notice %></p>

<h1>Listing Fourth Reviews</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Fourth test</th>
      <th>Video</th>
      <th>Likes2</th>
      <th>Aprendizaje2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fourth_reviews.each do |fourth_review| %>
      <tr>
        <td><%= fourth_review.user %></td>
        <td><%= fourth_review.fourth_test %></td>
        <td><%= fourth_review.video %></td>
        <td><%= fourth_review.likes2 %></td>
        <td><%= fourth_review.aprendizaje2 %></td>
        <td><%= link_to 'Show', fourth_review %></td>
        <td><%= link_to 'Edit', edit_fourth_review_path(fourth_review) %></td>
        <td><%= link_to 'Destroy', fourth_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fourth review', new_fourth_review_path %>
