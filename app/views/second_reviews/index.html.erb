<p id="notice"><%= notice %></p>

<h1>Listing Second Reviews</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Second test</th>
      <th>Problema1</th>
      <th>Cliente1</th>
      <th>Alternativa1</th>
      <th>Solucion1</th>
      <th>Ventajas1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_reviews.each do |second_review| %>
      <tr>
        <td><%= second_review.user %></td>
        <td><%= second_review.second_test %></td>
        <td><%= second_review.problema1 %></td>
        <td><%= second_review.cliente1 %></td>
        <td><%= second_review.alternativa1 %></td>
        <td><%= second_review.solucion1 %></td>
        <td><%= second_review.ventajas1 %></td>
        <td><%= link_to 'Show', second_review %></td>
        <td><%= link_to 'Edit', edit_second_review_path(second_review) %></td>
        <td><%= link_to 'Destroy', second_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second review', new_second_review_path %>
